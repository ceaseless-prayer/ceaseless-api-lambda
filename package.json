{
  "name": "ceaseless-api-lambda",
  "version": "0.0.1",
  "description": "A lambda function in NodeJs for the scripture APIs",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ceaseless-prayer/ceaseless-api-lambda.git"
  },
